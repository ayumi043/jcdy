var _admaster_callback = function (timer,url) {
    var us = [];
    if (window.sohuHD && sohuHD.pingback) {
        us = url.split('|');
        for (var i = 0; i < us.length; i++) {
            if (us[i].indexOf('admaster') > -1) {
                sohuHD.pingback(us[i] + escape(window.location.href));
            } else {
                sohuHD.pingback(us[i]);
            }
        }
    }
};
